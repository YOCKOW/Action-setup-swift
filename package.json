{
  "name": "Action-setup-swift",
  "version": "1.1.14",
  "description": "Setting Swift up.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "smoke-test": "jest"
  },
  "keywords": [
    "GitHub Actions",
    "Swift"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOCKOW/Action-setup-swift.git"
  },
  "author": "YOCKOW",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/semver": "^7.3.8",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
