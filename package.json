{
  "name": "Action-setup-swift",
  "version": "1.1.2-dev",
  "description": "Setting Swift up.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "smoke-test": "jest"
  },
  "keywords": [
    "GitHub Actions",
    "Swift"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOCKOW/Action-setup-swift.git"
  },
  "author": "YOCKOW",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.3",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^2.1.1",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.30",
    "@types/semver": "^7.1.0",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
