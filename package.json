{
  "name": "Action-setup-swift",
  "version": "1.1.8",
  "description": "Setting Swift up.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "smoke-test": "jest"
  },
  "keywords": [
    "GitHub Actions",
    "Swift"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOCKOW/Action-setup-swift.git"
  },
  "author": "YOCKOW",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^2.2.0",
    "esparse": "^0.6.11",
    "import-local": "^3.0.2",
    "jsesc": "^3.0.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.62",
    "@types/semver": "^7.3.4",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
